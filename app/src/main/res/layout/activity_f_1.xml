<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingPrefix"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/f_1_bg">

    <RelativeLayout
        android:id="@+id/showSettingMenu"
        android:layout_width="175.3dp"
        android:layout_height="147dp"
        android:layout_gravity="right"
        android:layout_marginRight="10dp"
        android:layout_marginTop="3dp"
        android:background="@drawable/s_bg"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/s_ll_rapoInfo"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/s_ll_rapoOrderSet"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/s_ll_rapoAlret"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="50dp">

            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
            android:id="@+id/f_1_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="false"
            android:layout_centerVertical="false"
            android:layout_marginTop="145dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/f_1_StartTime"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_centerInParent="false"
                android:text="00:00"
                android:textAlignment="center"

                android:textColor="@color/colorMainWhite"
                android:textSize="36.6sp"
                app:font="custom2" />

            <TextView
                android:id="@+id/f_1_RealStartTime"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerInParent="false"
                android:layout_marginLeft="35dp"
                android:layout_toRightOf="@+id/f_1_StartTime"
                android:text="00:00"

                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="36.6sp"
                app:font="custom2" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/f_1_rl3"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:layout_below="@+id/f_1_ll"
            android:layout_marginTop="125dp">

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/f_1_rl1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/f_1_rl3"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="false"
            android:gravity="center">

            <RelativeLayout
                android:id="@+id/f_1_bath_iv"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="false"
                android:layout_centerVertical="true"
                android:layout_marginRight="45dp"
                android:layout_weight="1"
                android:baselineAlignBottom="false"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="38.6dp"
                    android:layout_height="33.3dp"


                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    app:srcCompat="@drawable/f_1_bath_icon" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/f_1_massage_iv"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="false"
                android:layout_centerVertical="true"
                android:layout_marginRight="45dp"
                android:layout_toRightOf="@+id/f_1_bath_iv"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="21.3dp"
                    android:layout_height="40dp"

                    android:layout_centerHorizontal="true"
                    app:srcCompat="@drawable/f_1_massage_icon" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/f_1_book_iv"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentTop="true"
                android:layout_centerInParent="false"
                android:layout_centerVertical="true"
                android:layout_marginRight="45dp"
                android:layout_toEndOf="@+id/f_1_massage_iv"
                android:layout_weight="1"

                android:orientation="horizontal">

                <ImageView

                    android:layout_width="35.3dp"
                    android:layout_height="33.3dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    app:srcCompat="@drawable/f_1_book_icon" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/f_1_lullaby_iv"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:layout_toRightOf="@+id/f_1_book_iv"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView

                    android:layout_width="38dp"
                    android:layout_height="38dp"

                    android:layout_alignParentBottom="true"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    app:srcCompat="@drawable/f_1_lullaby_icon" />
            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/f_1_rl2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/f_1_rl1"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="false"
            android:layout_marginTop="10dp"
            android:gravity="center">

            <TextView
                android:id="@+id/f_1_lullaby_tv"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="false"
                android:layout_toRightOf="@+id/f_1_book_tv"
                android:text="+00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />

            <TextView
                android:id="@+id/f_1_massage_tv"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="45dp"
                android:layout_toRightOf="@+id/f_1_bath_tv"
                android:text="+00분"
                android:textAlignment="center"

                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />

            <TextView
                android:id="@+id/f_1_book_tv"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginRight="45dp"
                android:layout_toEndOf="@+id/f_1_massage_tv"
                android:text="+00분"
                android:textAlignment="center"

                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />

            <TextView
                android:id="@+id/f_1_bath_tv"
                android:layout_width="40dp"
                android:layout_height="wrap_content"

                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="45dp"
                android:baselineAlignBottom="false"
                android:text="+00분"
                android:textAlignment="center"

                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />
        </RelativeLayout>

        <Button
            android:id="@+id/f_1_btn"
            android:layout_width="256.6dp"
            android:layout_height="95dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/f_1_btn" />
    </RelativeLayout>
</FrameLayout>
