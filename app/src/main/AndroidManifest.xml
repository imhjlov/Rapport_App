<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rapport.rapport_app">

    <uses-feature
        android:name="android.hardware.bluetooth_le"
        android:required="true" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".Utils.App">

        <activity android:name=".LActivity"
            android:theme="@style/NoTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".S_1Activity" />
        <activity android:name=".S_2_1Activity" />
        <activity android:name=".S_3Activity" />
        <activity android:name=".S_2_2Activity" />
        <activity android:name=".m_Activity" />

        <service
            android:name=".Bluetooth.BluetoothLeService"
            android:enabled="true" />

        <activity android:name=".EActivity" />
        <activity android:name=".F_7_Activity" />
        <activity android:name=".F_7Activity" />
        <activity android:name=".F_1_Activity" />
        <activity android:name=".F_1Activity" />
        <activity android:name=".P_1Activity" />
        <activity android:name=".P_2Activity" />
        <activity android:name=".P_3Activity" />
        <activity android:name=".P_4Activity" />
        <activity android:name=".I_1Activity"/>
        <activity android:name=".I_2Activity"/>
        <activity android:name=".I_3Activity"/>
        <activity android:name=".I_4Activity"/>
        <activity android:name=".StartAlertActivity"
            android:theme="@style/NoTitle"/>
    </application>

</manifest>