<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout="@layout/activity_s_2_1"
    tools:ignore="MissingPrefix">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="43.6dp"
        android:background="@color/colorActionBar">

        <ImageButton
            android:id="@+id/backBtn"
            android:layout_width="19.3dp"
            android:layout_height="19.3dp"
            android:layout_centerVertical="true"
            android:background="@drawable/back_btn" />

        <TextView
            android:id="@+id/actionbar2_label"
            android:layout_width="160dp"
            android:layout_height="35dp"
            android:layout_centerInParent="true"
            android:gravity="center_vertical"
            android:textAlignment="center"
            android:textColor="@color/colorMainWhite"
            android:textSize="16.6sp"
            app:font="custom2" />

        <ImageButton

            android:id="@+id/s_setting"
            android:layout_width="19.6dp"
            android:layout_height="19.6dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/s_setting_btn" />

    </RelativeLayout>


</FrameLayout>