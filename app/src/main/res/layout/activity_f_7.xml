<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingPrefix"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/f_7_bg"
    tools:context="com.rapport.rapport_app.F_7Activity">

    <RelativeLayout
        android:id="@+id/showSettingMenu"
        android:layout_width="175.3dp"
        android:layout_height="147dp"
        android:layout_gravity="right"
        android:layout_marginRight="10dp"
        android:layout_marginTop="3dp"
        android:background="@drawable/s_bg"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/s_ll_rapoInfo"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/s_ll_rapoOrderSet"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/s_ll_rapoAlret"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="50dp">

            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/f_7_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="59dp"
            android:text="2017.00.00-00"
            android:textColor="@color/colorMainBlue"
            android:textSize="17sp"
            app:font="custom2" />

        <LinearLayout
            android:id="@+id/f_7_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/f_7_date"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="34dp"
            android:layout_marginRight="34dp"
            android:layout_marginTop="74dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/f_7_avg_start_time"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="36.6sp"
                app:font="custom2" />

            <TextView
                android:id="@+id/f_7_avg_total_time"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="36.6sp"
                app:font="custom2" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/f_7_rl1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/f_7_ll"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="false"
            android:layout_marginTop="125dp"
            android:gravity="center">

            <ImageView
                android:id="@+id/f_7_bath_iv"
                android:layout_width="69.3dp"
                android:layout_height="69.3dp"

                android:layout_marginRight="15dp"
                android:baselineAlignBottom="false"
                app:srcCompat="@drawable/f_7_bath_icon" />

            <ImageView
                android:id="@+id/f_7_massage_iv"
                android:layout_width="69.3dp"
                android:layout_height="69.3dp"
                android:layout_marginRight="15dp"
                android:layout_toRightOf="@+id/f_7_bath_iv"
                app:srcCompat="@drawable/f_7_massage_icon" />

            <ImageView
                android:id="@+id/f_7_book_iv"
                android:layout_width="69.3dp"
                android:layout_height="69.3dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="15dp"
                android:layout_toEndOf="@+id/f_7_massage_iv"
                app:srcCompat="@drawable/f_7_book_icon" />

            <ImageView
                android:id="@+id/f_7_lullaby_iv"
                android:layout_width="69.3dp"
                android:layout_height="69.3dp"
                android:layout_toRightOf="@+id/f_7_book_iv"
                app:srcCompat="@drawable/f_7_lullaby_icon" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/f_7_rl2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/f_7_rl1"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="false"
            android:layout_marginTop="10dp"
            android:gravity="center">

            <TextView
                android:id="@+id/f_7_bath_tv"
                android:layout_width="69.3dp"
                android:layout_height="wrap_content"

                android:layout_marginRight="15dp"
                android:baselineAlignBottom="false"
                android:text="00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />

            <TextView
                android:id="@+id/f_7_massage_tv"
                android:layout_width="69.3dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:layout_toRightOf="@+id/f_7_bath_tv"
                android:text="00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />

            <TextView
                android:id="@+id/f_7_book_tv"
                android:layout_width="69.3dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:layout_toRightOf="@+id/f_7_massage_tv"
                android:text="00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />

            <TextView
                android:id="@+id/f_7_lullaby_tv"
                android:layout_width="69.3dp"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/f_7_book_tv"
                android:text="00분"
                android:textAlignment="center"
                android:textColor="@color/colorMainWhite"
                android:textSize="15.3sp"
                app:font="custom1" />
        </RelativeLayout>

        <Button
            android:id="@+id/f_7_btn"
            android:layout_width="256.6dp"
            android:layout_height="95dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/f_7_btn" />
    </RelativeLayout>
</FrameLayout>
